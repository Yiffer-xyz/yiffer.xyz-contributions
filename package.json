{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "tailwindcss": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public",
    "dev": "concurrently \"npm run tailwindcss\" \"remix build && run-p dev:*\"",
    "start": "concurrently \"npm run tailwindcss\" \"remix build && run-p dev:*\""
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.14.0",
    "@remix-run/cloudflare-pages": "^1.14.0",
    "@remix-run/react": "^1.14.0",
    "@tsndr/cloudflare-worker-jwt": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "react": "^17.0.2",
    "react-calendar": "^4.0.0",
    "react-cropper": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-icons": "^4.7.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "^1.14.0",
    "@remix-run/eslint-config": "^1.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^17.0.45",
    "@types/react-calendar": "^3.9.0",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.1.2",
    "typescript": "^4.8.4",
    "wrangler": "^2.13.0"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}
