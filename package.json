  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "tailwindcss": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
